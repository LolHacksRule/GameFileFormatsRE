typedef enum <uint32>
{
    PX_RGB888 = 20,
    PX_RGBA8888 = 22,
    PX_RGBA8888_2, //X360 but may be CTR too
    PX_RGB888_2,
    PX_DXT_UNK = 64, //Confirm, def has the BC swizzle
    PX_DXT_UNK2 = 68, //ATI/BC4?
    PX_XBOX360_SWIZZLED_DXT_UNK = 80,
    PX_XBOX360_SWIZZLED_UNK2 = 84,
    PX_PSP2_SWIZZLED_ATI1L_UNK = 100,
    PX_PVRTC4 = 130,
    PX_PVRTC4A,
    PX_ETC2_RGB = 138, //Is this ETC1 or ETC2?
