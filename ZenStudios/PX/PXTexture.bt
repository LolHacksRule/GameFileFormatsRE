    PX_CTR_SWIZZLED_RGBA8888,
    PX_CTR_SWIZZLED_ETC1,
    PX_CTR_SWIZZLED_ETC1A4, 
    PX_PSP2_SWIZZLED_RGBA8888 = 192,
    PX_WIIU_SWIZZLED_RGBA8888 = 225,
    PX_WIIU_SWIZZLED_UNK = 240,
} PX_TexFmt;

struct ZenStudios_PinballXTexture
{
    LittleEndian();
    uint32 PXBinaryHdr;
    uint32 PXBinDataType;
    PX_TexFmt texFmt;
    uint32 width;
    uint32 height;
    uint32 mips;
    if (texFmt == 225 || texFmt == 240)
    {
        uint32 texSz;
        uint32 unk;
        byte tex[texSz];
    }
    //On WiiU, the off is read at 0x32
} PXT;